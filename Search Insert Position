#include<stdio.h>
int main()
{
    int nums[5] = {10,20,30,40,50};
    int target,count=0;
    printf("Enter element you want to search in array : ");
    scanf("%d",&target);
    for(int i=0;i<5;i++){
        if(target==nums[i]){
            printf("%d is found at index : %d",target,i);
            count++;
        }
    }
    if(count==0){
        printf("Element not found");
    }
    return 0;
}
