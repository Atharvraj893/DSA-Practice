#include<stdio.h>
int main()
{
    int temp;
    int n,value;
    printf("Enter size : ");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        printf("Enter value = ");
        scanf("%d",&value);
        arr[i] = value;
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n-1;j++)
        {
            if(arr[j]>arr[j+1])
            {
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
    if(n>=3)
    {
        printf("Third maximum : %d",arr[5-3]);
    }
    else{
        printf("Third maximum not exists so maximum value in array : %d",arr[n-1]);
    }
    return 0;
}
