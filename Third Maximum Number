#include<stdio.h>
int main()
{
    int temp;
    int arr[5] = {8,2,1,6,5};
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<4;j++)
        {
            if(arr[j]>arr[j+1])
            {
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
    printf("Third maximum : %d",arr[5-3]);
    // The third maximum element would be at size - 3 position here in this case our size=5 so 5-3
    return 0;
}
